  DataSet: TDataSet);
begin
  inherited;
 with qryReceteKirilim2 do
 begin
   Close;
   sql.Clear;
   sql.Add('SELECT * FROM ARF_W_ITEM_TREE WITH (NOLOCK) WHERE ITEM_ID=:ITEM_ID');
   Parameters.ParamByName('ITEM_ID').Value:=DataSet.FindField('MATERIAL_ID').Value;
   OPEN;
 end;
end;

